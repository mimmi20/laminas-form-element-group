{
  "name": "mimmi20/laminas-form-element-group",
  "description": "Provides an additional Form Element",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-group",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-group/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-group"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-form": "^3.21.0",
    "laminas/laminas-hydrator": "^4.16.0",
    "laminas/laminas-stdlib": "^3.20.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-i18n": "^2.30.0",
    "laminas/laminas-inputfilter": "^2.33.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-validator": "^2.64.1",
    "mimmi20/coding-standard": "^6.0.32",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.17",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpunit/phpunit": "^12.1.6",
    "rector/rector": "^2.0.16",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.3",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.9",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Element\\Group\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Element\\Group\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Element\\Group",
      "config-provider": "Mimmi20\\Form\\Element\\Group\\ConfigProvider"
    }
  }
}
