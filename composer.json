{
  "name": "mimmi20/laminas-form-element-group",
  "type": "library",
  "description": "Provides an additional Form Element",
  "homepage": "https://github.com/mimmi20/laminas-form-element-group",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-form": "^2.16.3",
    "laminas/laminas-hydrator": "^4.1.0",
    "laminas/laminas-inputfilter": "^2.12.0"
  },
  "require-dev": {
    "ext-intl": "*",
    "infection/infection": "^0.22.1",
    "laminas/laminas-i18n": "^2.11.1",
    "laminas/laminas-validator": "^2.14.4",
    "mimmi20/coding-standard": "^2.1.3",
    "pepakriz/phpstan-exception-rules": "^0.11.7",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.86",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.19",
    "phpunit/phpunit": "^9.5.4"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "required to use the Translator"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\Form\\Element\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Element\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Element\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-group/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-group"
  }
}
