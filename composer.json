{
  "name": "mimmi20/laminas-form-element-group",
  "description": "Provides an additional Form Element",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-form-element-group",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-form-element-group/issues",
    "source": "https://github.com/mimmi20/laminas-form-element-group"
  },
  "require": {
    "php": "^8.1.0",
    "laminas/laminas-form": "^3.14.0",
    "laminas/laminas-hydrator": "^4.15.0",
    "laminas/laminas-inputfilter": "^2.28.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.8",
    "laminas/laminas-i18n": "^2.24.1",
    "laminas/laminas-modulemanager": "^2.15.0",
    "laminas/laminas-validator": "^2.42.0",
    "mimmi20/coding-standard": "^5.1.24",
    "nikic/php-parser": "^v4.17.1",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.41",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpunit/phpunit": "^10.4.2",
    "rector/rector": "^0.18.6"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Form\\Element\\Group\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Form\\Element\\Group\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\Form\\Element\\Group",
      "config-provider": "Mimmi20\\Form\\Element\\Group\\ConfigProvider"
    }
  }
}
